Name: <input type="text" name="circlename" id="circlename"></input>
<input type="button" id="savecircle" name="savecircle"></input>

<script type="text/javascript">
$(document).ready(function(){
	$("#savecircle").click(function(){
		$.get('/circle/new', {name: $("#circlename").val()}, function(data){
			if(data == "OK"){
				console.log("circle successfully created!");
			} else if(data == "NOK"){
				console.log("error creating circle!");
			}
		});
	});
});
</script>