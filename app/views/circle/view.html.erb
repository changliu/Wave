			<div id = "grey" style="height: 320px; width: 100%;">
				<a class="nametxt">Friends in this Group: </a>
				<br/>
					<% users = @c.get_users(session[:user_id]) %>
					<% if users.length > 0 %>
						<ul id="using">
						<% for user in users %>
							<li class="use">
							<%= user.name %>
							</li>
							<% end %>
						</ul>
						<% else %>

						<p style=" margin-top:5px; margin-left:5px; font-family: HelveticaNeue-Light;">No users in this circle yet.</p>
					<% end %>
<br/><br/>
				<span class="create"><a class="yes" href="/circle/view_all">BACK</a>
				</span><br/>
				
			</div>
		</div>
